  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:test": "prettier \"projects/**/*.{ts,html,md,scss,json}\" --list-different",
    "format:wrtie": "prettier \"projects/**/*.{ts,html,md,scss,json}\" --write",
