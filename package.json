    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.5",
    "storybook": "^8.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.4.2",
    "webpack-bundle-analyzer": "^4.10.1"
